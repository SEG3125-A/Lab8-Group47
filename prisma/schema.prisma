// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource database {
  provider = "postgresql"
  url = env("DATABASE_LINK")
}

model User {
  id            Int       @id @default(autoincrement())
  email         String    @unique
  password      String      
  createdAt     DateTime  @default(now()) @map(name: "created_at")
  @@map(name: "users")
}